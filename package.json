{
  "name": "nestjs-tdl",
  "version": "0.0.0",
  "description": "TDLib module for NestJS",
  "keywords": [
    "nset",
    "nestjs",
    "nestjs-module",
    "telegram",
    "tdl",
    "tdlib",
    "tgl",
    "telegram-api",
    "telegram-client"
  ],
  "homepage": "https://github.com/Morb0/nestjs-tdl#readme",
  "bugs": {
    "url": "https://github.com/Morb0/nestjs-tdl/issues"
  },
  "author": "Morbo",
  "license": "MIT",
  "repository": "https://github.com/Morb0/nestjs-tdl.git",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "tdl": "6.2.0",
    "tdl-tdlib-addon": "0.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-angular": "11.0.0",
    "@nestjs/common": "7.6.3",
    "@nestjs/core": "7.6.3",
    "@types/node": "14.14.14",
    "@types/reflect-metadata": "0.1.0",
    "@typescript-eslint/eslint-plugin": "4.10.0",
    "@typescript-eslint/parser": "4.10.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.2.2",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write"
    ]
  }
}
